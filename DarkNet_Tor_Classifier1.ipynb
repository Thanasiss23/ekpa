      "source": [
        "# ===============================\n",
        "# 9. Εκπαίδευση Μοντέλων\n",
        "# ===============================\n",
        "models = {}\n",
        "if len(np.unique(y_train_res)) > 1:\n",
        "    models = {\n",
        "        \"Logistic Regression\": LogisticRegression(max_iter=1000, n_jobs=-1).fit(X_train_res_sc, y_train_res),\n",
        "        \"Random Forest\": RandomForestClassifier(\n",
        "            n_estimators=100, max_depth=10, random_state=42, n_jobs=-1\n",
        "        ).fit(X_train_res, y_train_res),\n",
        "        \"XGBoost\": xgb.XGBClassifier(\n",
        "            use_label_encoder=False, eval_metric='logloss',\n",
        "            max_depth=6, n_estimators=100, n_jobs=-1, random_state=42\n",
        "        ).fit(X_train_res, y_train_res)\n",
        "    }\n",
        "    print(\"Models trained successfully.\")\n",
        "else:\n",
        "    print(\"Skipping model training: y_train_res contains only one class.\")"
      ],
      "id": "6ww5h_NvE8oy"
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "5DT3Z8DvE8oz"
      },
      "outputs": [],
      "source": [
        "# ===============================\n",
        "# 10. Αξιολόγηση\n",
        "# ===============================\n",
        "results = {}\n",
        "for name, model in models.items():\n",
        "    if name == \"Logistic Regression\":\n",
        "        y_pred = model.predict(X_test_sc)\n",
        "    else:\n",
        "        y_pred = model.predict(X_test)\n",
        "    f1 = f1_score(y_test, y_pred)\n",
        "    results[name] = f1\n",
        "    print(\"\\n---\", name, \"---\")\n",
        "    print(\"F1-score:\", round(f1, 4))\n",
        "    print(classification_report(y_test, y_pred, target_names=[\"Non-Tor\",\"Tor\"]))"
      ],
      "id": "5DT3Z8DvE8oz"
