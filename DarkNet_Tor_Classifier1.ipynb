        "# ===============================\n",
        "# 8. Scaling\n",
        "# ===============================\n",
        "# Replace infinite values with NaN initially\n",
        "X_train_res.replace([np.inf, -np.inf], np.nan, inplace=True)\n",
        "X_test.replace([np.inf, -np.inf], np.nan, inplace=True)\n",
        "\n",
        "for col in X_train_res.columns:\n",
        "    col_median = X_train_res[col].median()\n",
        "    # Fill NaN values with the column median\n",
        "    X_train_res[col] = X_train_res[col].fillna(col_median)\n",
        "    X_test[col] = X_test[col].fillna(col_median)\n",
        "\n",
        "    # After filling NaNs, check for and replace any remaining infinite or too-large values\n",
        "    # This step is added to catch any edge cases not covered by the initial replacement\n",
        "    X_train_res[col] = X_train_res[col].replace([np.inf, -np.inf], col_median)\n",
        "    X_test[col] = X_test[col].replace([np.inf, -np.inf], col_median)\n",
        "\n",
        "\n",
        "# Cap all columns at the 95th percentile as a general measure against extreme values\n",
        "for col in X_train_res.columns:\n",
        "    cap_value = X_train_res[col].quantile(0.95)\n",
        "    X_train_res[col] = X_train_res[col].clip(upper=cap_value)\n",
        "    if col in X_test.columns:\n",
        "         X_test[col] = X_test[col].clip(upper=cap_value)\n",
        "\n",
        "# Display descriptive statistics after preprocessing but before scaling\n",
        "print(\"\\nDescriptive statistics of X_train_res after preprocessing:\")\n",
        "display(X_train_res.describe())\n",
        "\n",
        "\n",
        "# Check for columns with zero standard deviation before scaling\n",
        "cols_to_scale = [col for col in X_train_res.columns if X_train_res[col].std() != 0]\n",
        "\n",
        "scaler = StandardScaler()\n",
        "X_train_res_sc = scaler.fit_transform(X_train_res[cols_to_scale])\n",
        "X_test_sc = scaler.transform(X_test[cols_to_scale])"
      ],
